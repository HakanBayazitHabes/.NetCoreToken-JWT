
@{
    ViewData["Title"] = "GetAll";
}
@model List<ProductDto>


<h1>List</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Price</th>
            <th>UserId</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Price</td>
                <td>@item.UserId</td>
            </tr>
        }
    </tbody>
</table>

